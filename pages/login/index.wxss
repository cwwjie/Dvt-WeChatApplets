page {
  width: 100%;
  height: 100%;
  background: #f5f5f9;
}

/** 两翼留白 **/
.login .WingBlank {
  padding: 0rpx 35rpx;
}

.login .User-input {
  position: relative;
  margin-top: 20rpx;
  padding: 15rpx;
  border-radius: 10rpx;
  border: 1rpx solid #dbdbdb;
  background: #fff;
}

.login .User-input-error {
  padding-left: 15rpx;
  color: #F56C6C;
}

.login .User-input image {
  position: absolute;
  top: 27rpx;
  right: 35rpx;
  width: 53rpx;
  height: 32rpx;
  z-index: 2;
}

.login .User-automatiSave {
  display: block;
  padding-top: 25rpx;
  font-size: 14pt;
  color: #888888;
}

.login .User-submit {
  margin-top: 35rpx;
}
