page {
  width: 100%;
  height: 100%;
  background: #f5f5f9;
}

.giftInfor {
  position: relative;
  width: 100%;
  height: 100%;
}

.giftInfor .giftInfor-interval {
  margin-bottom: 35rpx;
}

/** description **/
.giftInfor .giftInfor-description {
  margin-bottom: 175rpx;
}

.giftInfor .giftInfor-extra-item {
  height: 0;
  overflow: hidden;
}

.giftInfor .giftInfor-extra-show {
  border-top: 1rpx solid #d9d9d9;
  overflow: visible;
  height: auto;
}

