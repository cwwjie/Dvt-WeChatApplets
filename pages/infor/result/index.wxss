page {
  background: #f5f5f9;
}

.result {
  position: relative;
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
}

.result .result-icon {
  padding-bottom: 35rpx;
  width: 100%;
  text-align: center;
}

.result .result-description {
  padding-bottom: 15rpx;
  width: 100%;
  text-align: center;
}

.result .result-infor {
  padding-bottom: 15rpx;
  width: 100%;
  font-size: 12pt;
  line-height: 20pt;
  text-align: center;
  color: #888888;
}

.result .result-button {
  padding-top: 20rpx;
  width: 100%;
  text-align: center;
}

.result .result-button .result-copy-error {
  border-color: #F56C6C;
  color: #F56C6C;
}

