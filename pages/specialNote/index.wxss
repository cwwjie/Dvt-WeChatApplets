page {
  background: #f5f5f9;
}

.specialNote {
  position: relative;
  width: 100%;
  height: 100%;
}

.specialNote .specialNote-interval {
  margin-bottom: 35rpx;
}

/** description **/
.specialNote .specialNote-description {
  margin-bottom: 175rpx;
}

.specialNote .specialNote-extra-item {
  height: 0;
  overflow: hidden;
}

.specialNote .specialNote-extra-show {
  border-top: 1rpx solid #d9d9d9;
  overflow: visible;
  height: auto;
}

/** operate **/
.specialNote .specialNote-operate {
  position: fixed;
  bottom: 0px;
  border-top: 1px solid #ddd;
  background: #fff;
  width: 100%;
}

.specialNote .specialNote-operate .WingBlank {
  padding: 20rpx 0rpx 10rpx;
}

.specialNote .specialNote-operate .btn-content {
  display: inline-block;
  padding: 0px 5%;
  width: 40%;
}
