page {
  width: 100%;
  height: 100%;
  background: #f5f5f9;
}

.roomInfor {
  position: relative;
  width: 100%;
  height: 100%;
}

.roomInfor .roomInfor-interval {
  margin-bottom: 35rpx;
}

/** main **/
.roomInfor .roomInfor-main {
  margin-bottom: 175rpx;
}

/** room **/
.roomInfor .main-room {
  padding-top: 20rpx;
}

.roomInfor .room-item {
  position: relative;
  background: #fff;
  border-radius: 5rpx;
  border: 1rpx solid #ddd;
}

.roomInfor .room-item-header {
  display: flex;
  align-items: center;
  padding: 20rpx;
}

.roomInfor .room-item-header image {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
}

.roomInfor .room-header-tiltle {
  padding-left: 15rpx;
}

.roomInfor .room-header-status {
  position: absolute;
  right: 35rpx;
}

.roomInfor .room-item-header .isComplete {
  color: #F56C6C;
}

/** operate **/
.roomInfor .roomInfor-operate {
  position: fixed;
  bottom: 0px;
  border-top: 1rpx solid #ddd;
  background: #fff;
  width: 100%;
}

.roomInfor .roomInfor-operate .WingBlank {
  padding: 20rpx 0rpx 10rpx;
}

.roomInfor .roomInfor-operate .btn-content {
  display: inline-block;
  padding: 0px 5%;
  width: 40%;
}
